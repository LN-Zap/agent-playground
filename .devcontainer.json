{
  "customizations": {
    "codespaces": {
      "openFiles": [
        "README.md"
      ],
      "repositories": {
        "LN-Zap/zap-skills": {
          "permissions": {
            "contents": "read"
          }
        }
      }
    },
    "vscode": {
      "extensions": [
        "github.copilot-chat",
        "arrterian.nix-env-selector",
        "jraylan.seamless-agent",
        "Rubymaniac.vscode-direnv"
      ]
    }
  },
  "image": "ghcr.io/cachix/devenv/devcontainer:latest",
  "name": "Agent Playground Dev Container",
  "overrideCommand": false,
  "postCreateCommand": "direnv allow",
  "secrets": {
    "GEMINI_API_KEY": {
      "description": "API key for Gemini access",
      "documentationUrl": "https://aistudio.google.com/api-keys"
    }
  },
  "updateContentCommand": "devenv test"
}
